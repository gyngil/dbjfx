<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="254.0" prefWidth="432.0" spacing="20.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.edu.pucgoias.dbjfx.PrimaryController">
   <children>
      <Label text="Primary View" />
      <Button fx:id="primaryButton" onAction="#switchToSecondary" text="Switch to Secondary View" />
      <Button fx:id="btnIncluir" mnemonicParsing="false" onAction="#incluirAluno" text="Incluir" />
      <Button fx:id="btnAlterar" mnemonicParsing="false" onAction="#alterarAluno" text="Alterar" />
      <Button fx:id="btnConsultar" mnemonicParsing="false" onAction="#consultarAluno" text="Consultar" />
      <Button fx:id="btnExcluir" mnemonicParsing="false" onAction="#excluirAluno" text="Excluir" />
      <Button fx:id="btnListar" mnemonicParsing="false" onAction="#listarAluno" text="Listar" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
